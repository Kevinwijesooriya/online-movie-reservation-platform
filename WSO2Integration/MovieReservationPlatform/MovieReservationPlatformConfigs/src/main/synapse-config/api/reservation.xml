<?xml version="1.0" encoding="UTF-8"?>
<api context="/reservation" name="reservation" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="GET" url-mapping="/getReservations">
        <inSequence>
            <log>
                <property name="Message" value="Welcome to Reservation"/>
            </log>
            <send>
                <endpoint key="getReservations"/>
            </send>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
